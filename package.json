{
  "name": "@aitmed/cadl",
  "version": "1.0.47",
  "description": "A package with utilities to manage and process CADL files.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "watch": "rollup -c -w",
    "test:start": "serve test_public",
    "start": "serve public",
    "dev": "npm-run-all --parallel start watch",
    "test:dev": "npm-run-all --parallel test:start watch",
    "test": "jest",
    "bump-version": "npm version patch",
    "prepublish:public": "npm run build && npm run bump-version",
    "publish:public": "npm publish --access public",
    "postpublish:public": "git push"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.aitmed.com/frontend/cadl.git"
  },
  "author": "Raymundo Martinez",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
