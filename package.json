{
  "name": "@aitmed/cadl",
  "version": "1.0.310",
  "description": "A package with utilities to manage and process CADL files.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "watch": "rollup -c -w",
    "serve:test": "serve test_public",
    "serve:val_ui": "serve public",
    "start:val_ui": "npm-run-all --parallel start:val_ui watch",
    "start:test": "npm-run-all --parallel serve:test watch ",
    "test": "jest",
    "test:CADL": "node --inspect node_modules/.bin/jest --watch --runInBand ./src/CADL/__tests__/ --watch",
    "test:db": "jest --verbose --watch --runInBand ./src/db --watch",
    "bump-version": "npm version patch -f",
    "prepublish:public": "npm run build && npm run bump-version",
    "publish:public": "npm publish -f",
    "postpublish:public": "git push"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.aitmed.com/frontend/cadl.git"
  },
  "author": "Raymundo Martinez",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^4.1.2",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.156",
    "babel-plugin-lodash": "^3.3.4",
    "core-js": "^3.6.5",
    "fs": "0.0.1-security",
    "jest-localstorage-mock": "^2.4.3",
    "lodash": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.10.7",
    "rollup-plugin-analyzer": "^3.2.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "serve": "^11.3.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.7"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "crypto-js": "^4.0.0",
    "dot-object": "^2.1.3",
    "elasticsearch": "^16.7.2",
    "humanize-duration": "^3.23.1",
    "immer": "^7.0.7",
    "jest": "^26.0.1",
    "jest-puppeteer": "^5.0.2",
    "jquery": "^3.5.1",
    "jsbi": "^3.1.4",
    "moment": "^2.27.0",
    "pako": "^1.0.11",
    "puppeteer": "^9.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "sql.js": "^1.5.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
